{
  "name": "code-review-agent",
  "version": "1.0.0",
  "description": "AI-Powered PR Reviewer for GitHub using Gemini 2.5 Pro",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "setup": "node scripts/setup.js",
    "build": "echo 'No build step required for Node.js project'",
    "health": "curl -f http://localhost:3000/health || exit 1"
  },
  "keywords": [
    "github",
    "code-review",
    "ai",
    "gemini",
    "webhook",
    "automation",
    "pull-request"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^19.0.13",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^8.1.0",
    "mongoose": "^8.17.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  }
}
